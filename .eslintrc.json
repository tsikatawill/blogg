{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": "plugin:react/recommended",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "sort-imports": "warn",
    "no-undef": "warn"
  }
}
